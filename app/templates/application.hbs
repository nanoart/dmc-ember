<div class="menubar-hoverable header-fixed {{if pinMode 'menubar-pin'}}">
	<!-- BEGIN HEADER-->
	<header id="header">
		{{#paper-toolbar}} {{#paper-toolbar-tools}}
		<h2>DualShield</h2><span class="flex-5"></span> {{#paper-button iconButton=true onClick=(action "togglePinMode")}} {{paper-icon
		"view list"}} {{/paper-button}}
		<span class="flex"></span> {{#power-select options=locales selected=defCountry onchange=(action "changeLocale") as |locale|}}
		{{locale}} {{/power-select}} {{#paper-button iconButton=true}}{{paper-icon "warning"}}{{/paper-button}} {{#paper-button}}{{paper-icon
		"message"}}{{t 'header.about'}}{{/paper-button}} {{#paper-button iconButton=true}}{{paper-icon "notifications"}}{{/paper-button}}
		{{#paper-button}}{{paper-icon "person"}}Logout{{/paper-button}} {{/paper-toolbar-tools}} {{/paper-toolbar}}
	</header>

	<div id="base">
		<div id="content">
			{{outlet}}
		</div>

		<!-- BEGIN MENUBAR-->
		<div id="menubar" class="{{if pinMode 'menubar-pin'}}  menubar-inverse">
			<div class="menubar-scroll-panel">

				<!-- BEGIN MAIN MENU -->
				<ul id="main-menu" class="gui-controls">

					<!-- BEGIN DASHBOARD -->
					<li class="{{if (eq 'dashboard' currentRouteName) 'active'}}">
						{{#link-to 'dashboard'}}
						<div class="gui-icon"><i class="fa fa-home" aria-hidden="true"></i></div>
						<span class="title">Dashboard</span>{{/link-to}}
					</li>
					<!--end /menu-li -->
					<!-- END DASHBOARD -->
					<li class="{{if (eq 'application-wizard.index' currentRouteName) 'active'}}">
						{{#link-to 'application-wizard.index'}}
						<div class="gui-icon"><i class="fa fa-magic" aria-hidden="true"></i></div>
						<span class="title">Application Wizard</span> {{/link-to}}
					</li>

					<li>
						<a>
							<div class="gui-icon"><i class="fa fa-user-secret" aria-hidden="true"></i></div>
							<span class="title">Agent</span>
						</a>
					</li>

					<!-- END EMAIL -->
					<li class="{{if (eq 'service-providers.index' currentRouteName) 'active'}}">
						{{#link-to 'service-providers.index'}}
						<div class="gui-icon"><i class="fa fa-server" aria-hidden="true"></i></div>
						<span class="title">{{t 'menubar.sps'}}</span> {{/link-to}}
					</li>

					<li class="{{if (eq 'users' currentRouteName) 'active'}}">
						{{#link-to 'users'}}
						<div class="gui-icon"><i class="fa fa-users" aria-hidden="true"></i></div>
						<span class="title">Users</span> {{/link-to}}
					</li>
					<li>
						{{#link-to 'tokens'}}
						<div class="gui-icon"><i class="fa fa-ticket" aria-hidden="true"></i></div>
						<span class="title">Tokens</span> {{/link-to}}
					</li>

					<li class="gui-folder {{if configured 'expanded'}}">
						<a {{action 'clickMenuItem' 'configured'}}>
							<div class="gui-icon"><i class="fa fa-cogs" aria-hidden="true"></i></div>
							<span class="title">Configuration</span>
						</a>
						<!--start submenu -->
						<ul>
							<li>{{#link-to 'license'}}<span class="title">License</span>{{/link-to}}</li>

							<li><a href="../../html/ui/typography.html"><span class="title">Roles</span></a></li>
							<li><a href="../../html/ui/cards.html"><span class="title">Policies</span></a></li>

							<li>{{#link-to 'gateways'}}<span class="title">Gateways</span>{{/link-to}}</li>


							<li><a href="../../html/ui/lists.html"><span class="title">networks</span></a></li>
							<li><a href="../../html/ui/tabs.html"><span class="title">Templates</span></a></li>
							<li><a href="../../html/ui/accordions.html"><span class="title">Syslog</span></a></li>
							<li><a href="../../html/ui/messages.html"><span class="title">OOBA</span></a></li>
							<li><a href="../../html/ui/offcanvas.html"><span class="title">Q&A</span></a></li>
							<li class="gui-folder">
								<a href="javascript:void(0);">
									<span class="title">Servers</span>
								</a>
								<!--start submenu -->
								<ul>
									<li><a href="../../html/ui/icons/materialicons.html"><span class="title">Biometric</span></a></li>
									<li><a href="../../html/ui/icons/fontawesome.html"><span class="title">Fail-through</span></a></li>
								</ul>
								<!--end /submenu -->
							</li>
							<!--end /menu-li -->
						</ul>
						<!--end /submenu -->
					</li>




					<li class="gui-folder {{if monitored 'expanded'}}">
						<a {{action 'clickMenuItem' 'monitored'}}>
							<div class="gui-icon"><i class="fa fa-binoculars" aria-hidden="true"></i></div>
							<span class="title">Monitor</span>
						</a>
						<!--start submenu -->
						<ul>
							<li><a href="../../html/ui/colors.html"><span class="title">Task</span></a></li>
							<li><a href="../../html/ui/typography.html"><span class="title">Audit</span></a></li>
							<li><a href="../../html/ui/cards.html"><span class="title">Report</span></a></li>
							<li><a href="../../html/ui/buttons.html"><span class="title">Alert</span></a></li>
						</ul>
						<!--end /submenu -->
					</li>					

					<!-- BEGIN UI -->
					<li class="gui-folder {{if advanced 'expanded'}}">
						<a {{action 'clickMenuItem' 'advanced'}}>
							<div class="gui-icon"><i class="fa fa-puzzle-piece fa-fw"></i></div>
							<span class="title">Advanced</span>
						</a>
						<!--start submenu -->
						<ul>
							<li><a href="../../html/ui/colors.html"><span class="title">Colors</span></a></li>
							<li><a href="../../html/ui/typography.html"><span class="title">Typography</span></a></li>
							<li><a href="../../html/ui/cards.html"><span class="title">Cards</span></a></li>
							<li><a href="../../html/ui/buttons.html"><span class="title">Buttons</span></a></li>
							<li><a href="../../html/ui/lists.html" class="active"><span class="title">Lists</span></a></li>
							<li><a href="../../html/ui/tabs.html"><span class="title">Tabs</span></a></li>
							<li><a href="../../html/ui/accordions.html"><span class="title">Accordions</span></a></li>
							<li><a href="../../html/ui/messages.html"><span class="title">Messages</span></a></li>
							<li><a href="../../html/ui/offcanvas.html"><span class="title">Off-canvas</span></a></li>
							<li><a href="../../html/ui/grid.html"><span class="title">Grid</span></a></li>
							<li class="gui-folder">
								<a href="javascript:void(0);">
									<span class="title">Icons</span>
								</a>
								<!--start submenu -->
								<ul>
									<li><a href="../../html/ui/icons/materialicons.html"><span class="title">Material Design Icons</span></a></li>
									<li><a href="../../html/ui/icons/fontawesome.html"><span class="title">Font Awesome</span></a></li>
									<li><a href="../../html/ui/icons/glyphicons.html"><span class="title">Glyphicons</span></a></li>
									<li><a href="../../html/ui/icons/skycons.html"><span class="title">Skycons</span></a></li>
								</ul>
								<!--end /submenu -->
							</li>
							<!--end /menu-li -->
						</ul>
						<!--end /submenu -->
					</li>
					<!--end /menu-li -->

					<li class="{{if (eq 'users' currentRouteName) 'active'}}">
						{{#link-to 'users'}}
						<div class="gui-icon"><i class="fa fa-stethoscope" aria-hidden="true"></i></div>
						<span class="title">System Diagram</span> {{/link-to}}
					</li>


					<!-- END UI -->
				</ul>
				<!-- END MAIN MENU -->

				<div class="menubar-foot-panel">
					<small class="no-linebreak hidden-folded">
				<span class="opacity-75">Copyright &copy; 2017</span> <strong>Deepnet Security</strong>
			</small>
				</div>

			</div>
		</div>
		<!-- END MAIN MENU -->

	</div>

</div>

